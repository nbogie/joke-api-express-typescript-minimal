        "test:watch": "vitest"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "rimraf": "^6.0.1",
        "tsx": "^4.16.2",
        "typescript": "^5.5.3",
        "vitest": "^2.0.2"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.19.2"
    }
}
